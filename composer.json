{
    "name": "make-it-app/laravel-user-roles",
    "description": "A package with role subsystem",
    "keywords": [
        "laravel", "user", "group", "grous", "role", "roles"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Kabin",
            "email": "ioffline@make-it.app"
        }
    ],
    "support": {
        "issues": "https://github.com/make-it-app/laravel-user-roles/issues",
        "source": "https://github.com/make-it-app/laravel-user-roles"
    },
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^9.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
      "psr-4": {
        "MakeIT\\UserRoles\\": "src/"
      },
      "files": [
        "src/helpers.php"
      ]
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.x-dev"
      },
      "laravel": {
        "providers": [
          "MakeIT\\UserRoles\\UserRolesServiceProvider"
        ]
      },
      "aliases": {
        "Role": "MakeIT\\UserRoles\\RoleFacade"
      }
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
